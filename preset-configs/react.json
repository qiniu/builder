{
  "extends": "esnext",
  "transforms": {
    "js": {
      "transformer": "jsx",
      "config": {
        "babelOptions": {
          "plugins": [
            ["@babel/plugin-proposal-decorators", { "legacy": true }],
            "@babel/plugin-proposal-class-properties"
          ]
        }
      }
    },
    "jsx": {
      "transformer": "jsx",
      "config": {
        "babelOptions": {
          "plugins": [
            ["@babel/plugin-proposal-decorators", { "legacy": true }],
            "@babel/plugin-proposal-class-properties"
          ]
        }
      }
    },
    "svg@jsx": "svg-sprite"
  }
}
